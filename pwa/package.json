  "scripts": {
    "dev-start": "webpack-dev-server --inline --progress --port 7010",
    "dev-stop-win": "\"for /f \"tokens=5\" %a in ('netstat -aon ^| findstr 8501 ^| findstr LISTENING') do taskkill /f /pid %a\"",
    "unit-test": "karma start",
    "e2e": "protractor protractor.conf.js",
    "bdd": "protractor protractor.cucumber.conf.js",
    "clean": "rimraf dist/*",
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "aspcore-build": "rimraf dist && webpack --config config/webpack.aspcore.prod.js --progress --profile --bail",
    "postinstall": "webdriver-manager update"
  },
